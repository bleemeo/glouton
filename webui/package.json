{
  "name": "glouton-panel",
  "version": "0.0.1",
  "description": "UI for local Glouton",
  "browserslist": [
    "defaults",
    "last 5 year",
    "firefox 45"
  ],
  "scripts": {
    "start": "vite --host 0.0.0.0",
    "build": "npm run clean && vite build --mode production",
    "deploy": "npm run build && mkdir -p ../api/assets/ && for f in $(find ./dist/assets -name '*.css'); do cat $f | gzip > ../api/assets/$(basename $f); done && for f in $(find ./dist/assets -name '*.js'); do cat $f | gzip > ../api/assets/$(basename $f); done",
    "clean": "rm -rf dist",
    "lint": "tsc -b && eslint src",
    "prettify": "prettier --write 'src/'"
  },
  "author": "Tom Darneix",
  "license": "ISC",
  "dependencies": {
    "@chakra-ui/react": "^3.25.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/lodash-es": "^4.17.12",
    "axios": "^1.11.0",
    "bootstrap": "^5.3.8",
    "core-js": "^3.45.1",
    "d3": "^7.9.0",
    "echarts": "^5.6.0",
    "lodash-es": "^4.17.21",
    "next-themes": "^0.4.6",
    "rc-switch": "^4.1.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.10",
    "react-debounce-input": "^3.3.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-router": "^7.6.3",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@types/d3": "^7.4.3",
    "@types/d3-time-format": "^4.0.3",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^4.7.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "mini-css-extract-plugin": "^2.9.4",
    "prettier": "^3.6.2",
    "sass": "1.89.2",
    "sass-embedded": "^1.91.0",
    "sass-loader": "16.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.3",
    "vite-plugin-env-compatible": "^2.0.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-html-purgecss": "^0.1.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
